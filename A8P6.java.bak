class A8P6
{
	public static void main(String[] args) 
	{
		int n = 5;
		int k = 0;

		for (int i = 0;i<n ;i++ )
		{
			
			for (int j = 0;j<n ;j++ )
			{
				
				if (i==(j+k))
				{
					System.out.print(n-k);
				}
				else
				{
					System.out.print(" ");
				}
							
			}

			System.out.println();
			k++;
		}
		
	}
}
